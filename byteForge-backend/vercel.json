{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_SERVICE_ROLE_KEY": "@supabase_service_role_key",
    "REMOVE_BG_KEY": "@remove_bg_key",
    "JWT_SECRET": "@jwt_secret",
    "ADMIN_PASSWORD": "@admin_password"
  }
}
